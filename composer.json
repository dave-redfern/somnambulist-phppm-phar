{
    "name": "somnambulist/php-pm-phar",
    "type": "library",
    "description": "Phar archive builder for php-pm",
    "license": "MIT",
    "require": {
        "php": ">=7",
        "ext-json": "*",
        "php-pm/php-pm": "^2.0",
        "php-pm/httpkernel-adapter": ">=2.0.4 <3.0",
        "symfony/console": "5.0.*",
        "symfony/config": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/finder": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "PHPPM\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
