{
    "name": "somnambulist/php-pm-phar",
    "type": "library",
    "description": "Phar archive builder for php-pm",
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": ">=7",
        "php-pm/php-pm": "dev-master",
        "php-pm/httpkernel-adapter": "dev-master",
        "php-pm/zend-adapter": "dev-master",
        "php-pm/drupal-adapter": "dev-master",
        "react/http": "dev-master",
        "symfony/debug": "3.3.13",
        "symfony/http-foundation": "3.3.13",
        "symfony/http-kernel": "3.3.13",
        "symfony/process": "3.3.13",
        "symfony/console": "3.3.13",
        "symfony/finder": "3.3.13"
    },
    "autoload": {
        "psr-4": {
            "PHPPM\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
